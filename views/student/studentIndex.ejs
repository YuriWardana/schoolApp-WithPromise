<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student List</title>
</head>
<body>
    <%- include("../partial/nav", {pageName: `Student's Datas`}) %>
   
    <table border="1">
        <tr>
            <a href="/students/add">add student</a>
            <th>Id</th>
            <th>first_name</th>
            <th>last_name</th>
            <th>email</th>
            <th>option</th>
        </tr>
        <% data.forEach(el =>{ %>
            <tr>
                <td><%= el.id %></td>
                <td><%= el.first_name %></td>
                <td><%= el.last_name %></td>
                <td><%= el.email %></td>
                <td> <a href="/students/<%= el.id %>/edit">edit</a>|<a href="/students/<%= el.id %>/delete">delete</a></td>
            </tr>
        <%  }) %>
    </table>
    
</body>
</html>